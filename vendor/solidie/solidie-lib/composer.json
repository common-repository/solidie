{
  "name": "solidie/solidie-lib",
  "version": "1.0.0",
  "description": "WP PHP scripts for plugins and themes",
  "type": "library",
  "autoload": {
    "psr-4": {
      "SolidieLib\\": "src/"
    }
  },
    "scripts": {
        "lint": "phpcs --extensions=php -s .",
        "lint:fix": "phpcbf --extensions=php ."
    },
	"require": {
	},
    "require-dev": {
        "10up/phpcs-composer": "dev-master"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
